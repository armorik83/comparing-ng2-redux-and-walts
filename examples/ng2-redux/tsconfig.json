{
    "compilerOptions": {
        "target": "es5",
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "outDir": "./.tmp"
    },
    "filesGlob": [
        "./**/*.ts",
        "!./**/*.d.ts",
        "!./node_modules/**/*",
        "./node_modules/typescript/lib/lib.es6.d.ts"
    ],
    "files": [
        "actions/index.ts",
        "components/Footer.ts",
        "components/Header.ts",
        "components/MainSection.ts",
        "components/TodoItem.ts",
        "components/TodoTextInput.ts",
        "constants/ActionTypes.ts",
        "constants/TodoFilters.ts",
        "containers/App.ts",
        "index.ts",
        "reducers/index.ts",
        "reducers/todos.ts",
        "store/configureStore.ts",
        "node_modules/typescript/lib/lib.es6.d.ts"
    ]
}