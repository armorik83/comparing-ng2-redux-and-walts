{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "es5",
            "es6"
        ],
        "noImplicitAny": true,
        "strictNullChecks": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "outDir": "./.tmp"
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "./**/*.d.ts",
        "node_modules"
    ]
}
